@import '../../assets/base'
$class = s($prefixer + '-play-icon-4-circle-rect-inner')

$slider-color = rgb(69,147,248)

$border-width = 60px
$circleDashConst1 = 154

common-animate(name)
  animation-duration 6s
  animation-timing-function linear
  animation-iteration-count infinite
  animation-fill-mode forwards
  animation-name name

.{$class}
  width 120px
  height 120px
  display flex

  .{$class}-svg
    width: 100%
    height 100%